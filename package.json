{
  "name": "linkmetur-platform",
  "version": "2.0.0",
  "private": true,
  "workspaces": ["frontend"],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "lint": "cd frontend && npm run lint",
    "dev:db": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:full": "npm run dev:db && npm run dev",
    "install:all": "npm install && cd frontend && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "db:generate": "cd frontend && npm run db:generate",
    "db:push": "cd frontend && npm run db:push",
    "db:seed": "cd frontend && npm run db:seed",
    "db:studio": "cd frontend && npm run db:studio"
  }
}