# Environment file para produção no Render
NODE_ENV=production

# Database (será preenchido automaticamente pelo Render)
DATABASE_URL=

# NextAuth configuration
NEXTAUTH_URL=https://linkmetur-frontend.onrender.com
NEXTAUTH_SECRET=

# JWT Secret
JWT_SECRET=

# Application URLs
FRONTEND_URL=https://linkmetur-frontend.onrender.com
BACKEND_URL=https://linkmetur-backend.onrender.com

# CORS Origins for production
CORS_ORIGINS=https://linkmetur-frontend.onrender.com,https://www.linkmetur.com

# Redis configuration (opcional para cache)
REDIS_URL=

# Email configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USER=
MAIL_PASS=

# Rate limiting for production
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# Cache TTL
CACHE_TTL=3600

# Security
BCRYPT_ROUNDS=12

# API Keys
GOOGLE_MAPS_API_KEY=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1