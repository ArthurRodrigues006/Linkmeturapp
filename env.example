# Configurações do Banco de Dados
DATABASE_URL=postgresql://username:password@localhost:5432/linkmetur
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=linkmetur_user
DB_PASSWORD=linkmetur_password
DB_DATABASE=linkmetur

# Configurações do Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Configurações de JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# Configurações de Email
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USER=your-email@gmail.com
MAIL_PASS=your-app-password

# Configurações dos Serviços
NODE_ENV=development
BACKEND_PORT=3001
FRONTEND_PORT=3000
LANDING_PORT=8081

# URLs dos Serviços
BACKEND_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000
LANDING_URL=http://localhost:8081

# Configurações de CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8081,https://app.linkmetur.com.br

# Configurações de Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=10

# Configurações de Cache
CACHE_TTL=300
